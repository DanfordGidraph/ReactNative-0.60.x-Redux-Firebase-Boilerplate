{
  "name": "BoilerplateApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "run:android": "react-native run-android",
    "run:ios": "react-native run-ios --simulator=\"iPhone X\"",
    "run:release": "react-native run-android --variant=release",
    "build:apk": "cd android && ./gradlew assembleRelease",
    "clean:android": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && cd android && ./gradlew clean",
    "test": "jest",
    "prepare": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-firebase/analytics": "^6.0.3",
    "@react-native-firebase/app": "6.0.3",
    "@react-native-firebase/crashlytics": "^6.0.3",
    "@react-native-firebase/perf": "^6.0.3",
    "axios": "^0.21.1",
    "lodash": "^4.17.19",
    "native-base": "^2.13.8",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/cli": "^2.9.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "patch-package": "^6.1.4",
    "postinstall-postinstall": "^2.0.0",
    "react-test-renderer": "16.8.6",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "redux-devtools-extension": "^2.13.8"    
  },
  "jest": {
    "preset": "react-native"
  }
}
